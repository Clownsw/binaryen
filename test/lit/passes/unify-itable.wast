  (func $itable-in-local (export "itable-in-local")
    (local $itable (ref null $itable))
    (local $itable-2 (ref null $itable))
    ;; Create more than one struct using the same itable. We need to propagate
    ;; the constant base for it to all gets of the local, and to do so through
    ;; the ref.as_non_nulls (that are necessary since the struct field is non-
    ;; nullable).
    (drop
      (struct.new $object
        (ref.as_non_null
          (local.tee $itable
            (global.get $itable)
          )
        )
      )
    )
    (drop
      (struct.new $object
        (ref.as_non_null
          (local.get $itable)
        )
      )
    )
    (drop
      (struct.new $object
        (ref.as_non_null
          (local.get $itable)
        )
      )
    )
    (drop
      (struct.new $object
        (ref.as_non_null
          ;; Tee to the local once more, and add another.
          (local.tee $itable-2
            (local.tee $itable
              (local.get $itable)
            )
          )
        )
      )
    )
    (drop
      (struct.new $object
        (ref.as_non_null
          (local.get $itable)
        )
      )
    )
    (drop
      (struct.new $object
        (ref.as_non_null
          (local.get $itable-2)
        )
      )
    )
    ;; Withut a tee.
    (local.set $itable
      (global.get $itable)
    )
    (drop
      (struct.new $object
        (ref.as_non_null
          (local.get $itable)
        )
      )
    )
  )

